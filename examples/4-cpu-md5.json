{
    "name": "Test 4 CPU md5",
    "project": "Funnel",
    "description": "My Desc",
    "inputs": [
        {
            "name": "infile",
            "description": "File to be MD5ed",
            "url": "file:///tmp/test_file",
            "type": "File",
            "path": "/tmp/test_file"
        }
    ],
    "outputs": [
        {
            "url": "file:///tmp/test_out_file",
            "type": "File",
            "path": "/tmp/test_out"
        }
    ],
    "resources": {
        "cpu_cores": 4
    },
    "executors": [
        {
            "image_name": "ubuntu",
            "cmd": ["md5sum", "/tmp/test_file"],
            "stdout": "/tmp/test_out",
            "stderr": "/tmp/test_err"
        }
    ]
}
